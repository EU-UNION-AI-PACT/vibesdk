{
	"name": "vibesdk-dev",
	"main": "worker/index.ts",
	"compatibility_date": "2025-08-10",
	"compatibility_flags": ["nodejs_compat"],
	"assets": {
		"directory": "dist/client",
		"not_found_handling": "single-page-application",
        "run_worker_first": false,
        "binding": "ASSETS"
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "vibesdk-db",
			"database_id": "c4721a2b-b96a-428a-8b2a-b3d255b307e9"
		}
	],
    "kv_namespaces": [
        {
            "binding": "VibecoderStore",
            "id": "local-dev"
        }
    ],
	"vars": {
        "TEMPLATES_REPOSITORY": "https://github.com/cloudflare/vibesdk-templates",
        "ALLOWED_EMAIL": "",
        "CUSTOM_DOMAIN": "localhost:5173",
        "MAX_SANDBOX_INSTANCES": "1",
        "SANDBOX_INSTANCE_TYPE": "standard",
        "DEV_MODE": "true"
    },
	"workers_dev": true
}